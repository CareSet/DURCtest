
{{#has_session_status}}
<div class='alert alert-info' role='alert'>
  {{session_status}}
</div>
{{/has_session_status}}

{{#is_new}}
<form action='/DURC/sibling' method='POST'>
{{/is_new}}

{{^is_new}}
<form action='/DURC/sibling/{{id}}' method='POST'>
    <input type='hidden' name='_method' value='PUT'>
{{/is_new}}

	<h1>sibling</h1>
<div class='card'>
  <div class='card-header'>
<h4> {{durc_instance_name}}</h4>
<ul>
<li> <a href='/DURC/sibling/'>Return to sibling list</a> </li>
{{^is_new}}
<li> <a href='/DURC/sibling/create'>Create new sibling </a> </li>
{{/is_new}}
</ul>
  </div>
<div class='card-body'>

  <fieldset>
    <legend>Edit {{durc_instance_name}}  </legend>
	<input type='hidden' name='_token' value='{{csrf_token}}'>
	
  <div class='form-group row'>
    <label for='id' class='col-sm-2 col-form-label'>id</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='id' name='id' placeholder='' value='{{id}}'>
    </div>
  </div>

  <div class='form-group row'>
    <label for='siblingname' class='col-sm-2 col-form-label'>siblingname</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='siblingname' name='siblingname' placeholder='' value='{{siblingname}}'>
    </div>
  </div>

  <div class='form-group row'>
    <label for='step_sibling_id' class='col-sm-2 col-form-label'>step_sibling_id</label>
    <div class='col-sm-10'>
	Current id value: {{step_sibling_id}} (see below for lookup value)<br>
	<select class='select2_step_sibling_id form-control' id='step_sibling_id' name='step_sibling_id' form-control'>
	<option value='{{step_sibling_id}}' selected='selected'>{{step_sibling_id}}</option>
	</select>
    </div>
  </div>

<script type='text/javascript'>

$('.select2_step_sibling_id').select2({
  ajax: {
    	url: '/DURC/searchjson/sibling/',
    	dataType: 'json'
  }
});


</script>


  <div class='form-group row'>
    <label for='sibling_id' class='col-sm-2 col-form-label'>sibling_id</label>
    <div class='col-sm-10'>
	Current id value: {{sibling_id}} (see below for lookup value)<br>
	<select class='select2_sibling_id form-control' id='sibling_id' name='sibling_id' form-control'>
	<option value='{{sibling_id}}' selected='selected'>{{sibling_id}}</option>
	</select>
    </div>
  </div>

<script type='text/javascript'>

$('.select2_sibling_id').select2({
  ajax: {
    	url: '/DURC/searchjson/sibling/',
    	dataType: 'json'
  }
});


</script>


  <div class='form-group row'>
    <label for='created_at' class='col-sm-2 col-form-label'>created_at</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='created_at' name='created_at' placeholder='' value='{{created_at}}'>

<button type='button' class='btn btn-primary' id='created_at_FontPicker'>
<img src='/images/DURC/ic_today_black_24dp_1x.png'> 
</button>
<button type='button' class='btn btn-primary' id='created_at_TodayNow'> Today and Now</button>

<script>
	var created_atConv = new AnyTime.Converter({format: '%Y-%m-%d %T'});
  	$('#created_at_TodayNow').click( function(e) {
      		$('#created_at').val(created_atConv.format(new Date())).change(); } );

    	$('#created_at_FontPicker').click(
      		function(e) {
        		$('#created_at').AnyTime_noPicker().AnyTime_picker(
				{
    					format: '%Y-%m-%d %T',
    					formatUtcOffset: '%: (%@)'
				} 
			).focus();
        	e.preventDefault();
        	} );
</script>

    </div>
  </div>

  <div class='form-group row'>
    <label for='updated_at' class='col-sm-2 col-form-label'>updated_at</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='updated_at' name='updated_at' placeholder='' value='{{updated_at}}'>

<button type='button' class='btn btn-primary' id='updated_at_FontPicker'>
<img src='/images/DURC/ic_today_black_24dp_1x.png'> 
</button>
<button type='button' class='btn btn-primary' id='updated_at_TodayNow'> Today and Now</button>

<script>
	var updated_atConv = new AnyTime.Converter({format: '%Y-%m-%d %T'});
  	$('#updated_at_TodayNow').click( function(e) {
      		$('#updated_at').val(updated_atConv.format(new Date())).change(); } );

    	$('#updated_at_FontPicker').click(
      		function(e) {
        		$('#updated_at').AnyTime_noPicker().AnyTime_picker(
				{
    					format: '%Y-%m-%d %T',
    					formatUtcOffset: '%: (%@)'
				} 
			).focus();
        	e.preventDefault();
        	} );
</script>

    </div>
  </div>

  <div class='form-group row'>
    <div class='col-sm-10'>
      <button type='submit' class='btn btn-primary'>Save Data</button>
    </div>
  </div>

</fieldset>
</div></div>
</form>

<br>
