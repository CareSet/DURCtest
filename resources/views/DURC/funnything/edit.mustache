{{^is_new}}
<form action='/DURC/funnything/{{id}}' method='POST'>
    <input type='hidden' name='_method' value='PUT'>
{{/is_new}}

	<h1>funnything</h1>
<div class='card'>
  <div class='card-header'>
<h4> {{durc_instance_name}}</h4>
<ul>
<li> <a href='/DURC/funnything/'>Return to funnything list</a> </li>
{{^is_new}}
<li> <a href='/DURC/funnything/create'>Create new funnything </a> </li>
{{/is_new}}
</ul>
  </div>
<div class='card-body'>

  <fieldset>
    <legend>Edit {{durc_instance_name}}  </legend>
	<input type='hidden' name='_token' value='{{csrf_token}}'>
	
  <div class='form-group row {{id_row_class}}'>
    <label for='id' class='col-sm-2 col-form-label'>id</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='id' name='id' placeholder='' value='{{id}}'>
    </div>
  </div>

  <div class='form-group row {{thisint_row_class}}'>
    <label for='thisint' class='col-sm-2 col-form-label'>thisint</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='thisint' name='thisint' placeholder='' value='{{thisint}}'>
    </div>
  </div>

  <div class='form-group row {{thisfloat_row_class}}'>
    <label for='thisfloat' class='col-sm-2 col-form-label'>thisfloat</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='thisfloat' name='thisfloat' placeholder='' value='{{thisfloat}}'>
    </div>
  </div>

  <div class='form-group row {{thisdecimal_row_class}}'>
    <label for='thisdecimal' class='col-sm-2 col-form-label'>thisdecimal</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='thisdecimal' name='thisdecimal' placeholder='' value='{{thisdecimal}}'>
    </div>
  </div>

  <div class='form-group row {{thisvarchar_row_class}}'>
    <label for='thisvarchar' class='col-sm-2 col-form-label'>thisvarchar</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='thisvarchar' name='thisvarchar' placeholder='' value='{{thisvarchar}}'>
    </div>
  </div>

  <div class='form-group row {{thisdate_row_class}}'>
    <label for='thisdate' class='col-sm-2 col-form-label'>thisdate</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='thisdate' name='thisdate' placeholder='' value='{{thisdate}}'>

<button type='button' class='btn btn-primary' id='thisdate_FontPicker'>
<img src='/images/DURC/ic_today_black_24dp_1x.png'> 
</button>
<button type='button' class='btn btn-primary' id='thisdate_TodayNow'> Today and Now</button>

<script>
	var thisdateConv = new AnyTime.Converter({format: '%Y-%m-%d'});
  	$('#thisdate_TodayNow').click( function(e) {
      		$('#thisdate').val(thisdateConv.format(new Date())).change(); } );

    	$('#thisdate_FontPicker').click(
      		function(e) {
        		$('#thisdate').AnyTime_noPicker().AnyTime_picker(
				{
    					format: '%Y-%m-%d',
    					formatUtcOffset: '%: (%@)'
				} 
			).focus();
        	e.preventDefault();
        	} );
</script>

    </div>
  </div>

  <div class='form-group row {{thisdatetime_row_class}}'>
    <label for='thisdatetime' class='col-sm-2 col-form-label'>thisdatetime</label>
    <div class='col-sm-10'>
