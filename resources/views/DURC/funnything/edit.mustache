<button type='button' class='btn btn-primary' id='thisdatetime_FontPicker'>
<img src='/images/DURC/ic_today_black_24dp_1x.png'> 
</button>
<button type='button' class='btn btn-primary' id='thisdatetime_TodayNow'> Today and Now</button>

<script>
	var thisdatetimeConv = new AnyTime.Converter({format: '%Y-%m-%d %T'});
  	$('#thisdatetime_TodayNow').click( function(e) {
      		$('#thisdatetime').val(thisdatetimeConv.format(new Date())).change(); } );

