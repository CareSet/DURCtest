      <input type='text' class='form-control' id='id' name='id' placeholder='' value='{{id}}'>
    </div>
  </div>

  <div class='form-group row {{productCode_row_class}}'>
    <label for='productCode' class='col-sm-2 col-form-label'>productCode</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='productCode' name='productCode' placeholder='' value='{{productCode}}'>
    </div>
  </div>

  <div class='form-group row {{productName_row_class}}'>
    <label for='productName' class='col-sm-2 col-form-label'>productName</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='productName' name='productName' placeholder='' value='{{productName}}'>
    </div>
  </div>

  <div class='form-group row {{description_row_class}}'>
    <label for='description' class='col-sm-2 col-form-label'>description</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='description' name='description' placeholder='' value='{{description}}'>
    </div>
  </div>

  <div class='form-group row {{standardCost_row_class}}'>
    <label for='standardCost' class='col-sm-2 col-form-label'>standardCost</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='standardCost' name='standardCost' placeholder='' value='{{standardCost}}'>
    </div>
  </div>

  <div class='form-group row {{listPrice_row_class}}'>
    <label for='listPrice' class='col-sm-2 col-form-label'>listPrice</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='listPrice' name='listPrice' placeholder='' value='{{listPrice}}'>
    </div>
  </div>

  <div class='form-group row {{reorderLevel_row_class}}'>
    <label for='reorderLevel' class='col-sm-2 col-form-label'>reorderLevel</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='reorderLevel' name='reorderLevel' placeholder='' value='{{reorderLevel}}'>
    </div>
  </div>

  <div class='form-group row {{targetLevel_row_class}}'>
    <label for='targetLevel' class='col-sm-2 col-form-label'>targetLevel</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='targetLevel' name='targetLevel' placeholder='' value='{{targetLevel}}'>
    </div>
  </div>

  <div class='form-group row {{quantityPerUnit_row_class}}'>
    <label for='quantityPerUnit' class='col-sm-2 col-form-label'>quantityPerUnit</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='quantityPerUnit' name='quantityPerUnit' placeholder='' value='{{quantityPerUnit}}'>
    </div>
  </div>

  <div class='form-group row {{discontinued_row_class}}'>
    <label for='discontinued' class='col-sm-2 col-form-label'>discontinued</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='discontinued' name='discontinued' placeholder='' value='{{discontinued}}'>
    </div>
  </div>

  <div class='form-group row {{minimumReorderQuantity_row_class}}'>
    <label for='minimumReorderQuantity' class='col-sm-2 col-form-label'>minimumReorderQuantity</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='minimumReorderQuantity' name='minimumReorderQuantity' placeholder='' value='{{minimumReorderQuantity}}'>
    </div>
  </div>

  <div class='form-group row {{category_row_class}}'>
    <label for='category' class='col-sm-2 col-form-label'>category</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='category' name='category' placeholder='' value='{{category}}'>
    </div>
  </div>

  <div class='form-group row {{attachments_row_class}}'>
    <label for='attachments' class='col-sm-2 col-form-label'>attachments</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='attachments' name='attachments' placeholder='' value='{{attachments}}'>
    </div>
  </div>

  <div class='form-group row'>
    <div class='col-sm-10'>
      <button type='submit' class='btn btn-primary'>Save Data</button>
    </div>
  </div>
  
  

</fieldset>
</div></div>
</form>

