{{^is_new}}
<form action='/DURC/product/{{id}}' method='POST'>
    <input type='hidden' name='_method' value='PUT'>
{{/is_new}}

	<h1>product</h1>
<div class='card'>
  <div class='card-header'>
<h4> {{durc_instance_name}}</h4>
<ul>
<li> <a href='/DURC/product/'>Return to product list</a> </li>
{{^is_new}}
<li> <a href='/DURC/product/create'>Create new product </a> </li>
{{/is_new}}
</ul>
  </div>
<div class='card-body'>

  <fieldset>
    <legend>Edit {{durc_instance_name}}  </legend>
	<input type='hidden' name='_token' value='{{csrf_token}}'>
	
  <div class='form-group row {{supplier_ids_row_class}}'>
    <label for='supplier_ids' class='col-sm-2 col-form-label'>supplier_ids</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='supplier_ids' name='supplier_ids' placeholder='' value='{{supplier_ids}}'>
    </div>
  </div>

  <div class='form-group row {{id_row_class}}'>
    <label for='id' class='col-sm-2 col-form-label'>id</label>
