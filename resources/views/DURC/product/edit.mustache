			<td>{{purchaseOrder_id_DURClabel}} ({{purchaseOrder_id}}) </td>			<td>{{customerOrder_id_DURClabel}} ({{customerOrder_id}}) </td>			<td>{{comments}}</td>
		
		{{/.}}
	</tr>
{{/inventorytransaction}}
</tbody>
</table>
</div></div> <!-- end inventorytransaction card-->
