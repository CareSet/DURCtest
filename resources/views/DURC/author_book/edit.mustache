
{{#has_session_status}}
<div class='alert alert-info' role='alert'>
  {{session_status}}
</div>
{{/has_session_status}}

{{#is_new}}
<form action='/DURC/author_book' method='POST'>
{{/is_new}}

{{^is_new}}
<form action='/DURC/author_book/{{id}}' method='POST'>
    <input type='hidden' name='_method' value='PUT'>
{{/is_new}}

	<h1>author_book</h1>
<div class='card'>
  <div class='card-header'>
<h4> {{durc_instance_name}}</h4>
<ul>
<li> <a href='/DURC/author_book/'>Return to author_book list</a> </li>
{{^is_new}}
<li> <a href='/DURC/author_book/create'>Create new author_book </a> </li>
{{/is_new}}
</ul>
  </div>
<div class='card-body'>

  <fieldset>
    <legend>Edit {{durc_instance_name}}  </legend>
	<input type='hidden' name='_token' value='{{csrf_token}}'>
	
  <div class='form-group row {{id_row_class}}'>
    <label for='id' class='col-sm-2 col-form-label'>id</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='id' name='id' placeholder='' value='{{id}}'>
    </div>
  </div>

  <div class='form-group row {{author_id_row_class}}'>
    <label for='author_id' class='col-sm-2 col-form-label'>author_id</label>
    <div class='col-sm-10'>
	Current id value: {{author_id}} (see below for lookup value)<br>
	<select class='select2_author_id form-control' id='author_id' name='author_id' form-control'>
	<option value='{{author_id}}' selected='selected'>{{author_id}}</option>
	</select>
    </div>
  </div>

<script type='text/javascript'>

$('.select2_author_id').select2({
  ajax: {
    	url: '/DURC/searchjson/author/',
    	dataType: 'json'
  }
});


</script>


  <div class='form-group row {{book_id_row_class}}'>
    <label for='book_id' class='col-sm-2 col-form-label'>book_id</label>
    <div class='col-sm-10'>
	Current id value: {{book_id}} (see below for lookup value)<br>
	<select class='select2_book_id form-control' id='book_id' name='book_id' form-control'>
	<option value='{{book_id}}' selected='selected'>{{book_id}}</option>
	</select>
    </div>
  </div>

<script type='text/javascript'>

$('.select2_book_id').select2({
  ajax: {
    	url: '/DURC/searchjson/book/',
    	dataType: 'json'
  }
});


</script>


  <div class='form-group row {{authortype_id_row_class}}'>
    <label for='authortype_id' class='col-sm-2 col-form-label'>authortype_id</label>
    <div class='col-sm-10'>
	Current id value: {{authortype_id}} (see below for lookup value)<br>
	<select class='select2_authortype_id form-control' id='authortype_id' name='authortype_id' form-control'>
	<option value='{{authortype_id}}' selected='selected'>{{authortype_id}}</option>
	</select>
    </div>
  </div>

<script type='text/javascript'>

$('.select2_authortype_id').select2({
  ajax: {
    	url: '/DURC/searchjson/authortype/',
    	dataType: 'json'
  }
});


</script>


  <div class='form-group row'>
    <div class='col-sm-10'>
      <button type='submit' class='btn btn-primary'>Save Data</button>
    </div>
  </div>

</fieldset>
</div></div>
</form>

<br>
