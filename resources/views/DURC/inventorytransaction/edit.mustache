{{#is_new}}
<form action='/DURC/inventorytransaction' method='POST'>
{{/is_new}}

{{^is_new}}
<form action='/DURC/inventorytransaction/{{id}}' method='POST'>
    <input type='hidden' name='_method' value='PUT'>
{{/is_new}}

	<h1>inventorytransaction</h1>
<div class='card'>
  <div class='card-header'>
<h4> {{durc_instance_name}}</h4>
<ul>
<li> <a href='/DURC/inventorytransaction/'>Return to inventorytransaction list</a> </li>
{{^is_new}}
<li> <a href='/DURC/inventorytransaction/create'>Create new inventorytransaction </a> </li>
{{/is_new}}
</ul>
  </div>
<div class='card-body'>

  <fieldset>
    <legend>Edit {{durc_instance_name}}  </legend>
	<input type='hidden' name='_token' value='{{csrf_token}}'>
	
  <div class='form-group row {{id_row_class}}'>
    <label for='id' class='col-sm-2 col-form-label'>id</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='id' name='id' placeholder='' value='{{id}}'>
    </div>
  </div>

  <div class='form-group row {{transactionType_row_class}}'>
    <label for='transactionType' class='col-sm-2 col-form-label'>transactionType</label>
    <div class='col-sm-10'>
	Current id value: {{transactionType}} (see below for lookup value)<br>
	<select class='select2_transactionType form-control' id='transactionType' name='transactionType' form-control'>
	<option value='{{transactionType}}' selected='selected'>{{transactionType}}</option>
	</select>
    </div>
  </div>

<script type='text/javascript'>

$('.select2_transactionType').select2({
  ajax: {
    	url: '/DURC/searchjson/inventoryTransactionType/',
    	dataType: 'json'
  }
});


</script>


  <div class='form-group row {{transactionCreatedDate_row_class}}'>
    <label for='transactionCreatedDate' class='col-sm-2 col-form-label'>transactionCreatedDate</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='transactionCreatedDate' name='transactionCreatedDate' placeholder='' value='{{transactionCreatedDate}}'>

<button type='button' class='btn btn-primary' id='transactionCreatedDate_FontPicker'>
<img src='/images/DURC/ic_today_black_24dp_1x.png'> 
</button>
<button type='button' class='btn btn-primary' id='transactionCreatedDate_TodayNow'> Today and Now</button>

<script>
	var transactionCreatedDateConv = new AnyTime.Converter({format: '%Y-%m-%d %T'});
  	$('#transactionCreatedDate_TodayNow').click( function(e) {
      		$('#transactionCreatedDate').val(transactionCreatedDateConv.format(new Date())).change(); } );

    	$('#transactionCreatedDate_FontPicker').click(
      		function(e) {
        		$('#transactionCreatedDate').AnyTime_noPicker().AnyTime_picker(
				{
    					format: '%Y-%m-%d %T',
    					formatUtcOffset: '%: (%@)'
				} 
			).focus();
        	e.preventDefault();
        	} );
</script>

    </div>
  </div>

  <div class='form-group row {{transactionModifiedDate_row_class}}'>
    <label for='transactionModifiedDate' class='col-sm-2 col-form-label'>transactionModifiedDate</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='transactionModifiedDate' name='transactionModifiedDate' placeholder='' value='{{transactionModifiedDate}}'>

<button type='button' class='btn btn-primary' id='transactionModifiedDate_FontPicker'>
<img src='/images/DURC/ic_today_black_24dp_1x.png'> 
</button>
<button type='button' class='btn btn-primary' id='transactionModifiedDate_TodayNow'> Today and Now</button>

<script>
	var transactionModifiedDateConv = new AnyTime.Converter({format: '%Y-%m-%d %T'});
  	$('#transactionModifiedDate_TodayNow').click( function(e) {
      		$('#transactionModifiedDate').val(transactionModifiedDateConv.format(new Date())).change(); } );

    	$('#transactionModifiedDate_FontPicker').click(
      		function(e) {
        		$('#transactionModifiedDate').AnyTime_noPicker().AnyTime_picker(
				{
    					format: '%Y-%m-%d %T',
    					formatUtcOffset: '%: (%@)'
				} 
			).focus();
        	e.preventDefault();
        	} );
</script>

    </div>
  </div>

  <div class='form-group row {{product_id_row_class}}'>
    <label for='product_id' class='col-sm-2 col-form-label'>product_id</label>
    <div class='col-sm-10'>
	Current id value: {{product_id}} (see below for lookup value)<br>
	<select class='select2_product_id form-control' id='product_id' name='product_id' form-control'>
	<option value='{{product_id}}' selected='selected'>{{product_id}}</option>
	</select>
    </div>
  </div>

<script type='text/javascript'>

$('.select2_product_id').select2({
  ajax: {
    	url: '/DURC/searchjson/product/',
    	dataType: 'json'
  }
});


</script>


  <div class='form-group row {{quantity_row_class}}'>
    <label for='quantity' class='col-sm-2 col-form-label'>quantity</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='quantity' name='quantity' placeholder='' value='{{quantity}}'>
    </div>
  </div>

  <div class='form-group row {{purchaseOrder_id_row_class}}'>
    <label for='purchaseOrder_id' class='col-sm-2 col-form-label'>purchaseOrder_id</label>
    <div class='col-sm-10'>
	Current id value: {{purchaseOrder_id}} (see below for lookup value)<br>
	<select class='select2_purchaseOrder_id form-control' id='purchaseOrder_id' name='purchaseOrder_id' form-control'>
	<option value='{{purchaseOrder_id}}' selected='selected'>{{purchaseOrder_id}}</option>
	</select>
    </div>
  </div>

<script type='text/javascript'>

$('.select2_purchaseOrder_id').select2({
  ajax: {
    	url: '/DURC/searchjson/purchaseOrder/',
    	dataType: 'json'
  }
});


</script>


  <div class='form-group row {{customerOrder_id_row_class}}'>
    <label for='customerOrder_id' class='col-sm-2 col-form-label'>customerOrder_id</label>
    <div class='col-sm-10'>
	Current id value: {{customerOrder_id}} (see below for lookup value)<br>
	<select class='select2_customerOrder_id form-control' id='customerOrder_id' name='customerOrder_id' form-control'>
	<option value='{{customerOrder_id}}' selected='selected'>{{customerOrder_id}}</option>
	</select>
    </div>
  </div>

<script type='text/javascript'>

$('.select2_customerOrder_id').select2({
  ajax: {
    	url: '/DURC/searchjson/order/',
    	dataType: 'json'
  }
});


</script>


  <div class='form-group row {{comments_row_class}}'>
    <label for='comments' class='col-sm-2 col-form-label'>comments</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='comments' name='comments' placeholder='' value='{{comments}}'>
    </div>
  </div>

  <div class='form-group row'>
    <div class='col-sm-10'>
      <button type='submit' class='btn btn-primary'>Save Data</button>
    </div>
  </div>

</fieldset>
</div></div>
</form>

<br>
