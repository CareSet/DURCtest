
{{#has_session_status}}
<div class='alert alert-info' role='alert'>
  {{session_status}}
</div>
{{/has_session_status}}

{{#is_new}}
<form action='/DURC/salesreport' method='POST'>
{{/is_new}}

{{^is_new}}
<form action='/DURC/salesreport/{{id}}' method='POST'>
    <input type='hidden' name='_method' value='PUT'>
{{/is_new}}

	<h1>salesreport</h1>
<div class='card'>
  <div class='card-header'>
<h4> {{durc_instance_name}}</h4>
<ul>
<li> <a href='/DURC/salesreport/'>Return to salesreport list</a> </li>
{{^is_new}}
<li> <a href='/DURC/salesreport/create'>Create new salesreport </a> </li>
{{/is_new}}
</ul>
  </div>
<div class='card-body'>

  <fieldset>
    <legend>Edit {{durc_instance_name}}  </legend>
	<input type='hidden' name='_token' value='{{csrf_token}}'>
	
  <div class='form-group row'>
    <label for='groupBy' class='col-sm-2 col-form-label'>groupBy</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='groupBy' name='groupBy' placeholder='' value='{{groupBy}}'>
    </div>
  </div>

  <div class='form-group row'>
    <label for='display' class='col-sm-2 col-form-label'>display</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='display' name='display' placeholder='' value='{{display}}'>
    </div>
  </div>

  <div class='form-group row'>
    <label for='title' class='col-sm-2 col-form-label'>title</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='title' name='title' placeholder='' value='{{title}}'>
    </div>
  </div>

  <div class='form-group row'>
    <label for='filterRowSource' class='col-sm-2 col-form-label'>filterRowSource</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='filterRowSource' name='filterRowSource' placeholder='' value='{{filterRowSource}}'>
    </div>
  </div>

  <div class='form-group row'>
    <label for='default' class='col-sm-2 col-form-label'>default</label>
    <div class='col-sm-10'>
      <input type='text' class='form-control' id='default' name='default' placeholder='' value='{{default}}'>
    </div>
  </div>

  <div class='form-group row'>
    <div class='col-sm-10'>
      <button type='submit' class='btn btn-primary'>Save Data</button>
    </div>
  </div>

</fieldset>
</div></div>
</form>

