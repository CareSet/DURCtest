<button type='button' class='btn btn-primary' id='random_date_FontPicker'>
<img src='/css/ic_today_black_24dp_1x.png'> 
</button>
<button type='button' class='btn btn-primary' id='random_date_TodayNow'> Today and Now</button>

<script>
	var random_dateConv = new AnyTime.Converter({format: '%Y-%m-%d %T'});
  	$('#random_date_TodayNow').click( function(e) {
      		$('#random_date').val(random_dateConv.format(new Date())).change(); } );

    	$('#random_date_FontPicker').click(
      		function(e) {
        		$('#random_date').AnyTime_noPicker().AnyTime_picker(
				{
    					format: '%Y-%m-%d %T',
    					formatUtcOffset: '%: (%@)'
				} 
			).focus();
        	e.preventDefault();
        	} );
</script>

